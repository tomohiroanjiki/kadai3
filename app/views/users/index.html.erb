<%= render 'users/list', user: @user, book: @book %>
<div class='col-md-8 offset-md-1'>
<h3>Users</h3>
<table class='table'>
<thead>
  <tr>
    <th>image</th>
    <th>name</th>
    <th colspan="3"></th>
  </tr>
</thead>
<% @users.each do |user| %>
  <thead>
    <tr>
      <th><%= attachment_image_tag current_user, :profile_image, format: 'jpeg', fallback: "sample-author1.jpg", size: "40x40" %></th>
      <th><%= user.name %></th>
      <th><%= link_to 'Show', user_path(user) %></th>
    </tr>
  </thead>
<% end %>
</div>
